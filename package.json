{
  "name": "tagged_gallery",
  "version": "1.0.0",
  "engines": {
    "node": "7.9.0"
  },
  "description": "Gallery of images which allow assigning multiple tags to an image",
  "main": "index.js",
  "scripts": {
    "prestart": "nsp check",
    "postinstall": "npm run build",
    "build:server": "webpack --config webpack.server.config.js",
    "build:client": "webpack --config webpack.client.config.js",
    "build": "npm run build:server && npm run build:client",
    "start": "nodemon --watch src --exec npm run build:and:start",
    "start:prod": "node ./dist/server/index.js",
    "start:dev": "node --debug ./dist/server/index.js",
    "build:and:start": "npm run build && npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ivan Iakimov",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.25",
    "node-fetch": "^1.6.3",
    "webpack": "^2.4.1"
  }
}
