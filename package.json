{
  "name": "tagged_gallery",
  "version": "1.0.0",
  "engines": {
    "node": "7.10.1"
  },
  "description": "Gallery of images which allow assigning multiple tags to an image",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build:server": "webpack --config webpack.server.config.js",
    "build:client": "webpack --config webpack.client.config.js",
    "build": "npm-run-all --parallel build:server build:client",
    "start": "nodemon --ext js,jsx,css,html --watch src --exec npm run build:and:start",
    "start:prod": "node ./dist/server/index.js",
    "start:dev": "node --inspect ./dist/server/index.js",
    "build:and:start": "npm run build && npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ivan Iakimov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.9",
    "node-fetch": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.1",
    "whatwg-fetch": "^3.0.0"
  }
}
