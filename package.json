{
  "name": "tagged_gallery",
  "version": "1.0.0",
  "engines": {
    "node": "7.9.0"
  },
  "description": "Gallery of images which allow assigning multiple tags to an image",
  "main": "index.js",
  "scripts": {
    "prestart": "nsp check",
    "postinstall": "npm run build",
    "build:server": "webpack --config webpack.server.config.js",
    "build:client": "webpack --config webpack.client.config.js",
    "build": "npm-run-all --parallel build:server build:client",
    "start": "nodemon --ext js,jsx,css,html --watch src --exec npm run build:and:start",
    "start:prod": "node ./dist/server/index.js",
    "start:dev": "node ./dist/server/index.js",
    "build:and:start": "npm run build && npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ivan Iakimov",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.25",
    "node-fetch": "^1.6.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-tagsinput": "^3.16.1",
    "redux": "^3.6.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "whatwg-fetch": "^2.0.3"
  }
}
