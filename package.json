{
  "name": "tagged_gallery",
  "version": "1.0.0",
  "engines": {
    "node": "12.6.0"
  },
  "description": "Gallery of images which allow assigning multiple tags to an image",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build:server": "webpack --config webpack.server.config.js --mode production",
    "build:client": "webpack --config webpack.client.config.js --mode production",
    "build": "npm-run-all --parallel build:server build:client",
    "heroku-postbuild": "echo Skip builds on Heroku",
    "start": "nodemon --ext js,jsx,css,html --watch src --exec npm run build:and:start",
    "start:prod": "node ./dist/server/index.js",
    "start:dev": "node --inspect ./dist/server/index.js",
    "build:and:start": "npm run build && npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ivan Iakimov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.2.7",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.4",
    "whatwg-fetch": "^3.0.0"
  }
}
