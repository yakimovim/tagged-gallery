{
  "name": "tagged_gallery",
  "version": "1.1.0",
  "engines": {
    "node": "16.9.1"
  },
  "description": "Gallery of images which allow assigning multiple tags to an image",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build:server": "webpack --config webpack.server.config.js --mode=production",
    "build:client": "webpack --config webpack.client.config.js --mode=production",
    "build": "npm-run-all --parallel build:server build:client",
    "heroku-postbuild": "echo Skip builds on Heroku",
    "start": "nodemon --ext js,jsx,css,html --watch src --exec npm run build:and:start",
    "start:prod": "node ./dist/server/index.js",
    "start:dev": "node --inspect ./dist/server/index.js",
    "build:and:start": "npm run build && npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ivan Iakimov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.1",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.67.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "mongodb": "^4.3.1",
    "node-fetch": "^3.2.0",
    "preact": "^10.6.4",
    "prop-types": "^15.8.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.1.2",
    "saslprep": "^1.0.3",
    "whatwg-fetch": "^3.6.2"
  }
}
