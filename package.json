{
  "name": "tagged_gallery",
  "version": "1.0.0",
  "engines": {
    "node": "12.6.0"
  },
  "description": "Gallery of images which allow assigning multiple tags to an image",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build:server": "webpack --config webpack.server.config.js --mode=production",
    "build:client": "webpack --config webpack.client.config.js --mode=production",
    "build": "npm-run-all --parallel build:server build:client",
    "heroku-postbuild": "echo Skip builds on Heroku",
    "start": "nodemon --ext js,jsx,css,html --watch src --exec npm run build:and:start",
    "start:prod": "node ./dist/server/index.js",
    "start:dev": "node --inspect ./dist/server/index.js",
    "build:and:start": "npm run build && npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ivan Iakimov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.3",
    "node-fetch": "^2.6.1",
    "preact": "^10.5.7",
    "prop-types": "^15.7.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.5",
    "whatwg-fetch": "^3.5.0"
  }
}
